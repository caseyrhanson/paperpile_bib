@ARTICLE{Jung2020-mx,
  title    = "How to interpret and integrate multi-omics data at systems level",
  author   = "Jung, Gun Tae and Kim, Kwang-Pyo and Kim, Kwoneel",
  abstract = "Current parallel sequencing technologies generate biological
